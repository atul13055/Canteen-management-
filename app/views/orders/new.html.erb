
<%= form_with model: @order, url: menu_card_user_orders_path, method: :post do |form| %>
  <div>
    <%= form.label :name %>:- 
    <%= current_user.name%>
  </div><br>
  <div>
    <%= form.label :email %>:-  
    <%= current_user.email %>
  </div><br>

  <div>
    <%= form.label :qntity  %>
    <%= form.number_field :qnty %>
  </div><br>

  <div>
    <%= form.submit %>
  </div>
<% end %>
